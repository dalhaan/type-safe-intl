{
  "name": "type-safe-intl",
  "version": "0.1.0",
  "description": "A starter boilerplate project for building a Node tool with Rollup.",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/es/index.mjs",
      "default": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "author": "Dallan Freemantle",
  "license": "MIT",
  "repository": "https://github.com/dalhaan/type-safe-intl",
  "private": true,
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm src/cli.ts",
    "build": "yarn clean && tsc -p . && rollup -c",
    "test": "jest",
    "clean": "rm -rf types lib dist",
    "check:lint": "eslint src/**/*.ts",
    "check:types": "tsc --noEmit",
    "check:all": "npm-run-all --parallel check:lint check:types"
  },
  "packageManager": "yarn@1.22.19",
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/node": "^18.11.16",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "esbuild": "^0.16.10",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.7.5",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17.0 || ^18.0"
  }
}
